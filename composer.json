{
    "name": "myaza-software/schema-validator",
    "type": "symfony-bundle",
    "description": "Validate json,xml",
    "authors": [
        {
            "name": "Vlad Shashkov",
            "email": "root@myaza.info"
        }
    ],
    "autoload": {
        "psr-4": {
            "SchemaValidator\\": "src/"
        }
    },
    "require": {
        "php": ">=8",
        "symfony/dependency-injection": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/property-access": "^5.2",
        "symfony/property-info": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/serializer": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "config": {
        "sort-packages": true
    }
}
